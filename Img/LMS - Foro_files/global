<input type="hidden" id="g-Recaptcha-Response" name="g-Recaptcha-Response">
    <script>
        var captchaForm = document.getElementById('g-Recaptcha-Response').form;
        var elements = captchaForm.getElementsByTagName('input');
        var submitButton;
        for (var i = 0; i < elements.length; i++) {
            if (elements[i].type.toLowerCase() === "submit") {
                if (document.getElementById('g-Recaptcha-Response').value == '') {
                    submitButton = elements[i];
                    submitButton.setAttribute('disabled', '');
                    submitButton.setAttribute('sc-current-text', submitButton.value);
                    submitButton.value = 'Cargando...';
                }
            }
        }

        var formBeingSubmitted = false;
        captchaForm.onsubmit = function () {
            if (formBeingSubmitted) {
                return false;
            }
            submitButton.value = 'Cargando...';
            formBeingSubmitted = true;
            submitButton.setAttribute('disabled', '');
            submitButton.setAttribute('sc-form-submitted', 'true');

            return true;
        };

    </script>
    <script src="https://www.google.com/recaptcha/enterprise.js?render=6LefMrMaAAAAAO3OCNNnrVeTmx041qN6x5arXRRI"></script>
    <script>
        grecaptcha.enterprise.ready(recaptcha);

        function recaptcha() {
            grecaptcha.enterprise
                .execute('6LefMrMaAAAAAO3OCNNnrVeTmx041qN6x5arXRRI', { action: 'Login' })
                .then(function (token) {
                    var captchaForm = document.getElementById('g-Recaptcha-Response').form;
                    var elements = captchaForm.getElementsByTagName('input');
                    for (var i = 0; i < elements.length; i++) {
                        var input = elements[i];
                        if (input.type.toLowerCase() === "submit") {
                            if (input.getAttribute('sc-form-submitted') != 'true') {
                                input.removeAttribute('disabled');
                                var oldValue = input.getAttribute('sc-current-text');
                                if (oldValue != '') {
                                    input.value = input.getAttribute('sc-current-text');
                                }
                            }
                            break;
                        }
                    }
                    document.getElementById('g-Recaptcha-Response').value = token;
                    clearTimeout(recaptcha);
                    setTimeout(recaptcha, 100000);
                });
        }
    </script>
